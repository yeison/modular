from utils.index import Index


fn print_tensor_info[gpu: Bool = False](tensor: LayoutTensor):
    print("---tensor-begin---")

    @parameter
    if not gpu:
        print("layout: ", tensor.layout)
    print("runtime_layout: ", tensor.runtime_layout)
    print("address_space: ", Int(tensor.address_space))
    print("values:")
    print(tensor)
    print("---tensor-end---")


