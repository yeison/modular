    alias M = 512
    var f32_simd = SIMD[DType.float32, M](0.0)

    for i in range(M):
        f32_simd[i] = i - 256

    f32_casted_e5m2 = f32_simd.cast[DType.float8_e5m2]()

    for i in range(64):
        for j in range(8):
            print(f32_casted_e5m2[i * 8 + j], end=", ")
        print("")


fn test_simd_float8[
    type: DType,
    size: Int,
    target: DType,
](x: SIMD[type, size]):
    var x_casted = x.cast[target]()

    alias M = 32
    alias N = size // M
    for i in range(M):
        for j in range(N):
            print(x_casted[i * N + j], end=", ")
        print("")


