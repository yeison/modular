    a: UnsafePointer[NoneType],
    a_type: DataType,
    lda: Int32,
    b: UnsafePointer[NoneType],
    b_type: DataType,
    ldb: Int32,
    beta: UnsafePointer[NoneType],
    c: UnsafePointer[NoneType],
    c_type: DataType,
    ldc: Int32,
