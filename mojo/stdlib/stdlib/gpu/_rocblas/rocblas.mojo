fn rocblas_ssyr2k(
    handle: Handle,
    uplo: Fill,
    trans: Operation,
    n: Int32,
    k: Int32,
    alpha: UnsafePointer[Float32],
    _a: UnsafePointer[Float32],
    lda: Int32,
    _b: UnsafePointer[Float32],
    ldb: Int32,
    beta: UnsafePointer[Float32],
    _c: UnsafePointer[Float32],
    ldc: Int32,
