fn rocblas_sgemv_strided_batched(
    handle: Handle,
    trans_a: Operation,
    m: Int32,
    n: Int32,
    alpha: UnsafePointer[Float32],
    _a: UnsafePointer[Float32],
    lda: Int32,
    stride_a: Int64,
    x: UnsafePointer[Float32],
    incx: Int32,
