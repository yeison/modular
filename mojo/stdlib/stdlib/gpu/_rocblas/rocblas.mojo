fn rocblas_ssyrk_strided_batched(
    handle: Handle,
    uplo: Fill,
    trans_a: Operation,
    n: Int32,
    k: Int32,
    alpha: UnsafePointer[Float32],
    _a: UnsafePointer[Float32],
    lda: Int32,
    stride__a: Int64,
    beta: UnsafePointer[Float32],
    _c: UnsafePointer[Float32],
