load("//bazel:api.bzl", "mojo_test")

[
    mojo_test(
        name = src + ".test",
        srcs = [src],
        tags = ["skip-external-ci-macos-15"],  # TODO: Debug why this only fails on public github actions, not locally
        deps = [
            "@mojo//:quantization",
            "@mojo//:stdlib",
        ],
    )
    for src in glob(["**/*.mojo"])
]
